/**index.wxss**/


page {
  display: block;
  min-height: 100%;
  background-color: rgba(6, 148, 124, 0.219);
}
.pro-item{
  overflow: hidden;
  padding: 40rpx;
  border-bottom: 1rpx solid rgba(94, 104, 119, 0.24);
}

.pro-logo{
  float: left;
  width: 200rpx;
  height: 200rpx;
}

.pro-body{
  margin-left: 250rpx;
}

.pro-title{
  color: rgb(56, 94, 138);
  font-size: 34rpx;
  line-height: 1;
}

.pro-desc{
  color: rgb(43, 36, 61);
  font-size: 26rpx;
  line-height: 1.2;
}




